  data: {
    returnData:function({_req}){
        var a = {
            '10':'联系组1',
            '11':'联系组2',
            '12':'联系组3'
        }
        return {
            ..._req.body,
            id:Math.ceil(Math.random()*10000),
            strGroup: (_req.body.group||[]).map( item => a[item+''] )
        };
    }
  }
}
